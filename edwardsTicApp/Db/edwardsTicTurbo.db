    field(LLSV, "MAJOR")
}
record(mbbi, "$(P)TURBO:SPEED:PRI")
{
    field(ZRST, "OK")
    field(ONST, "Warning")
    field(ONSV, "MINOR")
    field(TWST, "Alarm")
    field(TWSV, "MINOR")
    field(THST, "Alarm")
    field(THSV, "MAJOR")
}
