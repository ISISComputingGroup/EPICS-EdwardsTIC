    field(DESC, "Speed")
    field(DTYP, "stream")
    field(INP,  "@edwardsTic.proto get_t_speed($(P)TURBO:SPEED:) $(PORT)")
    field(SCAN, "1 second")
    field(EGU,  "%")
    field(PREC, "1")
    field(LOPR, "0.0")
    field(HOPR, "110.0")
    field(HIGH, "110.0")
    field(LOW,  "0.0")
    field(HIHI, "110.1")
    field(LOLO, "-0.1")
    field(HSV,  "MINOR")
    field(LSV,  "MINOR")
    field(HHSV, "MAJOR")
    field(LLSV, "MAJOR")
}
